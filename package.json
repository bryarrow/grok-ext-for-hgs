{
  "name": "grok-ext-for-hgs",
  "displayName": "HydroGeoSphere Language Grammar VScode extension",
  "description": "The extension  add HydroGeoSphere .grok file support ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "grok",
      "aliases": ["grok", "grok"],
      "extensions": [".grok"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "grok",
      "scopeName": "source.grok",
      "path": "./syntaxes/grok.tmLanguage.json"
    }]
  }
}
